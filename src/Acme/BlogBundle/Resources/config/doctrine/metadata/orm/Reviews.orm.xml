<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Reviews" table="reviews">
    <indexes>
      <index name="fk_review_user_idx" columns="reviews_user_id"/>
    </indexes>
    <id name="reviewsId" type="integer" column="reviews_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="reviewsDescription" type="text" column="reviews_description" nullable="false"/>
    <field name="reviewsMark" type="boolean" column="reviews_mark" nullable="false"/>
    <many-to-one field="reviewsUser" target-entity="User">
      <join-columns>
        <join-column name="reviews_user_id" referenced-column-name="user_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
