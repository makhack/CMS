<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Orderuserproduct" table="orderuserproduct">
    <indexes>
      <index name="fk_orderuserproduct_userproduct_idx" columns="orderuserproduct_userProduct_id"/>
      <index name="fk_orderuserproduct_order_idx" columns="orderuserproduct_order_id"/>
    </indexes>
    <id name="orderuserproductId" type="integer" column="orderuserproduct_id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="orderuserproductUserproduct" target-entity="Userproduct">
      <join-columns>
        <join-column name="orderuserproduct_userProduct_id" referenced-column-name="userProduct_id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="orderuserproductOrder" target-entity="Order">
      <join-columns>
        <join-column name="orderuserproduct_order_id" referenced-column-name="order_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
